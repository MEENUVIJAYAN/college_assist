        {{/if}}
    </div>
  </header>
  <main id="main">


  <!-- ======= Breadcrumbs Section ======= -->
  <section class="breadcrumbs">
    <div class="container" id="blur">
         <div class="d-flex justify-content-between align-items-center c1">
        <h2 class="tb">Attendance</h2>
         </div>
      {{#if alert}}
      <div class="alert alert-danger alert-dismissible fade show mt-3" style="border-radius: 10px" role="alert">
        <strong>Alert!</strong> Incorrect credentials
      </div>
      {{/if}}

      <div class="table responsive mt-5 d-none d-md-flex">
        {{#if editable}}

        <table class="table table-striped zoom roundedTable t1">
          <thead class="tab1">
               <tr>
              <th width="5%" scope="col">Sl.No</th>
              <th width="10%" scope="col">Name</th>
              <th width="7%" scope="col">Roll No</th>
              <th width="7%" scope="col">Semester</th>
              <th width="7%" scope="col">Sub1</th>
              <th width="7%" scope="col">Sub2</th>
              <th width="7%" scope="col">Sub3</th>
              <th width="7%" scope="col">Sub4</th>
              <th width="7%" scope="col">Sub5</th>
            </tr>
          </thead>
          <tbody>
            {{#each data }}
             <tr>
              <th scope="row">{{this.index}}</th>
              <td>{{this.Username}}</td>
              <td>{{this.RollNo}}</td>
              <td>{{this.Semester}}</td>
                 <td>{{this.Att1}}</td>
                 <td>{{this.Att2}}</td>
                 <td>{{this.Att3}}</td>
                 <td>{{this.Att4}}</td>
                 <td>{{this.Att5}}</td>
                
            </tr>
            {{/each}}
          </tbody>
        </table>
        {{else}}
        <table class="table table-striped zoom roundedTable t1">
          <thead class="tab1">
            <tr>
              <th width="5%" scope="col">Sl.No</th>
              <th width="10%" scope="col">Name</th>
              <th width="7%" scope="col">Roll No</th>
              <th width="7%" scope="col">Semester</th>
              <th width="7%" scope="col">Sub1</th>
              <th width="7%" scope="col">Sub2</th>
              <th width="7%" scope="col">Sub3</th>
              <th width="7%" scope="col">Sub4</th>
              <th width="7%" scope="col">Sub5</th>
            </tr>
          </thead>
          <tbody>
            {{#each data }}
            <tr>
             <th scope="row">{{this.index}}</th>
              <td>{{this.Username}}</td>
              <td>{{this.RollNo}}</td>
              <td>{{this.Semester}}</td>
                 <td>{{this.Att1}}</td>
                 <td>{{this.Att2}}</td>
                 <td>{{this.Att3}}</td>
                 <td>{{this.Att4}}</td>
                 <td>{{this.Att5}}</td>
                
            </tr>
            {{/each}}
          </tbody>
        </table>
        {{/if}}
      </div>

      <section id="card" class="d-flex d-md-none">
        {{#if editable}}

        {{#each data }}

        <div class="card-deck col-sm-3  mt-3">
          <div class="text-center">
            <div class="card cardv1 ">
              <div class="card-body">
                <h3 class="card-title mb-3">{{this.Username}}</h3>
                <p>{{this.RollNo}}</p>
                <p>{{this.Semester}}</p>
                 <p>{{this.Att1}}</p>
                 <p>{{this.Att2}}</p>
                 <p>{{this.Att3}}</p>
                 <p>{{this.Att4}}</p>
                 <p>{{this.Att5}}</p>

              



              </div>
            </div>

          </div>
        </div>
        {{/each}}

        {{else}}

        {{#each data }}
         <div class="card-deck col-sm-3  mt-3">
          <div class="text-center">
            <div class="card cardv1 ">
              <div class="card-body">
               <h3 class="card-title mb-3">{{this.Username}}</h3>
                <p>{{this.RollNo}}</p>
                <p>{{this.Semester}}</p>
                 <p>{{this.Att1}}</p>
                 <p>{{this.Att2}}</p>
                 <p>{{this.Att3}}</p>
                 <p>{{this.Att4}}</p>
                 <p>{{this.Att5}}</p>

                
              </div>
            </div>
          </div>
          </div>
                {{/each}}

                {{/if}}


  </section><!-- End Breadcrumbs Section -->

</main><!-- End #main -->