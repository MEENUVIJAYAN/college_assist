<header>
    <nav>
      <div id="navbar1">
        <div id="logo1" class="reverse">
          <div class="site-logo mr-auto w-20 ml-5">GECB</div>

        </div>
        <div id="links1">
           {{#if logout1}}
        <li class="cta1"><a class="nav-link" href="/it-user"><span>Back</span></a></li>
        {{else}}
        <li class="cta1"><a class="nav-link" href="/EEE-user"><span>Back</span></a></li>
        {{/if}}
      </div>

    </nav>
    <!-- Mobile Menu -->
    <div id="mySidenav" class="sidenav">
      <a style="cursor:pointer;" class="closebtn" onclick="closeNav()">&times;</a>
         {{#if logout1}}
        <li class="cta1"><a class="nav-link" href="/it-user"><span>Back</span></a></li>
